{
  "name": "excel-clone",
  "version": "1.0.0",
  "description": "Excel clone with Node.js backend",
  "main": "server.js",
  "bin": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "pkg . --targets node16-win-x64 --output excel-clone.exe && node_modules\\rcedit\\bin\\rcedit.exe excel-clone.exe --set-requested-execution-level requireAdministrator"
  },
  "dependencies": {
    "cors": "^2.8.5",
